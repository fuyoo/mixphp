{
  "name": "mix/grpc",
  "description": "gRPC library based on Swoole coroutine, including protoc code generator, server, and client\n",
  "type": "library",
  "keywords": [
    "mix",
    "swoole",
    "coroutine",
    "grpc",
    "protoc",
    "server",
    "client"
  ],
  "homepage": "http://www.mixphp.cn/",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "liu,jian",
      "email": "coder.keda@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "ext-swoole": ">=4.4.4",
    "google/protobuf": "^3.11",
    "mix/runtime": "~2.2.0",
    "mix/http-message": "~2.2.0",
    "mix/http-server": "~2.2.0",
    "mix/micro-server": "~2.2.0"
  },
  "autoload": {
    "psr-4": {
      "Mix\\Grpc\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0.0"
  },
  "autoload-dev": {
    "psr-4": {
      "GPBMetadata\\": "protos/GPBMetadata/",
      "Php\\": "protos/Php/"
    }
  }
}
